@model List<StokTakipSistemi.Models.tblUrun>
@{
    Layout = null;
}
<table class="table table-hover">
   <tr>
    <th>Ürün Adı</th>
    <th>Satın Alma Tarihi</th>
    <th>Toplam Fiyatı</th>
    <th>Parça tipi</th>
    <th>Satın Alınan Kurum</th>
    <th></th>
    </tr>
    @foreach(var urun in Model)
    {
        <tr>
            <td>@urun.urunAd</td>
            <td>@urun.urunSatinAlmaTarih</td>
            <td>@urun.urunFiyat<span class="simge-tl"> &#8378;</span></td>
            <td>@urun.tblUrunGenel.tblParcaTipi.parcaTipi</td>
            <td>@urun.satinAlinanKurum</td>
             <td>
               
                 <a class="btn btn-warning" href="/AdminPersonel/ZimmetKaldir/@urun.urunId">Zimmet Kaldır</a>
             </td>
        </tr>



    }

</table>
